
# Setting PATH for Python 3.7
# The original version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/3.7/bin:${PATH}"
export PATH


export CLICOLOR=1
export LSCOLORS=gxbxbxbxfxbxbxfxfxgxgx
export PS1="\[\033[33m\]\w\[\033[m\]\$ "
#export PS1="\[\033[90m\]\h:\[\033[33m\]\W\[\033[m\]\$ "
#export PS1="\e[0;34m\]\u\[\e[m\]@\[\e[0;32m\]\h\[\e[m\]:\[\e[0;33m\]\w\[\e[m\]$ "
#export PS1="\e[0;32m\]\h\[\e[m\]:\[\e[0;33m\]\w\[\e[m\]$ "
alias ls='ls -GFh' 
